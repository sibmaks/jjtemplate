{
  "definitions": [
    {
      "baseDate": "{{ '28.10.2024' | parseDate 'dd.MM.yyyy' }}",
      "usLocale": "{{ locale 'en', 'US' }}",
      "ruLocale": "{{ locale 'ru', 'RU' }}"
    }
  ],
  "template": {
    "usKey": "{{ formatDate .usLocale, 'dd MMMM yyyy', .baseDate }}",
    "usPipeKey": "{{ .baseDate | formatDate .usLocale, 'dd MMMM yyyy' }}",
    "ruKey": "{{ formatDate .ruLocale, 'dd MMMM yyyy', .baseDate }}",
    "ruPipeKey": "{{ .baseDate | formatDate .ruLocale, 'dd MMMM yyyy' }}"
  }
}